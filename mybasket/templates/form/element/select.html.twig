<select name="{{ name }}" class="form-control">
    {% if empty %}
        {% include "form/element/option.html.twig" with {
        'value': '',
        'label': empty
        } %}
    {% endif %}
    {% for option in options %}
        {% include "form/element/option.html.twig" with {
        'value': option.value,
        'current': current,
        'label': option.label
        } %}
    {% endfor %}
</select>